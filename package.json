{
  "name": "sso-with-oauth-and-saml",
  "version": "0.1.0",
  "description": "CCS sixth session goes over jwt, Oauth2, and SAML",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony app.js",
    "debug": "node --inspect app.js",
    "unit:test": "ava --verbose test/unit-tests/*.js",
    "integration:test": "ava --verbose test/integration-tests/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "e2e": "node --harmony test/nightwatch.runner.js",
    "test": "nyc ava test/integration-tests/*.js test/unit-tests/*.js",
    "posttest": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Code-Craftsmanship-Saturdays/sso-with-oauth-and-saml.git"
  },
  "keywords": [
    "nodejs",
    "jwt",
    "oauth",
    "saml",
    "authentication",
    "authorization"
  ],
  "author": "Jean-Marcel Belmont",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Code-Craftsmanship-Saturdays/sso-with-oauth-and-saml/issues"
  },
  "homepage": "https://github.com/Code-Craftsmanship-Saturdays/sso-with-oauth-and-saml#readme",
  "dependencies": {
    "dotenv": "^2.0.0",
    "jsonwebtoken": "^7.2.1",
    "passport-saml-restify": "^1.0.5",
    "chromedriver": "^2.25.1",
    "restify": "^4.3.0",
    "restify-jwt": "^0.4.0",
    "rethinkdb": "^2.3.3",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "nightwatch": "^0.9.12",
    "nock": "^9.0.2",
    "nyc": "^10.0.0",
    "selenium-server": "^3.0.1",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1",
    "tap-nyan": "^1.1.0",
    "testdouble": "^1.11.0"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "exclude": [
      "test/end-to-end-tests/*",
      "test/helpers/*",
      "test/nightwatch.conf.js",
      "test/nightwatch.runner.js"
    ]
  }
}
